  <plugin name="com.appnovation.ppw-camera" source="pgb" spec="1.0.36" />
  
  <!--SQLite plugin-->
  <!--<plugin spec="https://github.com/litehelpers/Cordova-sqlite-storage.git#1.2.2" source="git" />-->
  <plugin name="cordova-sqlite-evcore-extbuild-free" spec="0.9.0"/>
  
  <!--Device function permissions-->
  <plugin name="cordova.plugins.diagnostic" spec="4.0.3"/>
  
  <!--File Opener-->
  <!--<plugin spec="https://github.com/pwlin/cordova-plugin-file-opener2.git" source="git" />-->
  <plugin name="cordova-plugin-file-opener2" spec="2.0.5"/>
  <!--<plugin name="cordova-plugin-fileopener"/>-->
	
  <!--Mobile Accessibility-->
  <plugin spec="https://github.com/phonegap/phonegap-mobile-accessibility.git" source="git" />   
   
  <!--App Version-->
  <plugin spec="https://github.com/whiteoctober/cordova-plugin-app-version.git#0.1.8" source="git" />   
  
  <!--Disable the NSURL Cache on iOS-->
  <plugin spec="https://github.com/wongatech/cordova-disable-nsurl-cache.git" source="git" /> 
    
  <!--Camera Preview-->
  <plugin name="cordova-plugin-camera-preview" spec="https://github.com/WeaponX86/cordova-plugin-camera-preview.git" />
  <!--<plugin name="cordova-plugin-camera-preview" spec="https://github.com/cordova-plugin-camera-preview/cordova-plugin-camera-preview.git" />-->
  
  <!--File Hashing-->
  <plugin spec="https://github.com/Abdsoft/phonegap-hash.git" />
  
  <!-- Define resources for each platform. -->

  
  <platform name="android">
    <icon src="resources/android/icon/drawable-ldpi-icon.png" platform="android" qualifier="ldpi" density="ldpi" />
